			<result name="success">/jsp/myOrder.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="allocateSeat" class="com.action.UserAction" method="allocateSeat">
			<result name="success">/jsp/Plan.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="activate" class="com.action.UserAction" method="activate">
			<result name="success">/jsp/UserLogin.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
