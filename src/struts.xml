<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" 
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.i18n.encoding" value="UTF-8" />
	<package name="struts2" extends="struts-default">

		<action name="addUser" class="com.action.UserAction" method="addUser">
			<result name="success">/index.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>

		<action name="changeCurOrder" class="com.action.UserAction" method="changeCurOrder">
			<result name="success">/jsp/pay.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>


		<action name="modifyUserInfo" class="com.action.UserAction" method="modifyUserInfo">
			<result name="success">/jsp/MainFrame.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>


		<action name="cancelOrder" class="com.action.UserAction" method="cancelOrder">
			<result name="success">/jsp/myOrder.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="getOrder" class="com.action.UserAction" method="getOrder">
			<result name="success">/jsp/myOrder.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>

		<action name="activate" class="com.action.UserAction" method="activate">
			<result name="success">/jsp/UserLogin.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="getActDetail" class="com.action.UserAction" method="getActDetail">
			<result name="success">/jsp/activity.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="SearchByCond" class="com.action.UserAction" method="SearchByCond">
			<result name="success">/jsp/search.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="SearchByInp" class="com.action.UserAction" method="SearchByInp">
			<result name="success">/jsp/search.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="CreateOrder" class="com.action.UserAction" method="CreateOrder">
			<result name="success">/jsp/order_check.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="Pay" class="com.action.UserAction" method="Pay">
			<result name="success">/jsp/myOrder.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="login" class="com.action.UserAction" method="login">
			<result name="index">/index.jsp</result>
			<result name="search">/jsp/search.jsp</result>
			<result name="activity">/jsp/activity.jsp</result>
		</action>
		<action name="logout" class="com.action.UserAction" method="logout">
			<result name="success">/index.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
		<action name="ActPageChange" class="com.action.UserAction" method="ActPageChange">
			<result name="success">/jsp/search.jsp</result>
			<result name="error">/jsp/Fail.jsp</result>
		</action>
	</package>
</struts>