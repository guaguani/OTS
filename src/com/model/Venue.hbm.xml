<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.model.Venue" table="venue" catalog="tickets">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="assigned"></generator>
        </id>
        <property name="password" type="java.lang.String">
            <column name="password" length="45"/>
        </property>
        <property name="address" type="java.lang.String">
            <column name="address" length="45"/>
        </property>
        <property name="totalseats" type="java.lang.Integer">
            <column name="totalseats" length="45"/>
        </property>
        <property name="rows" type="java.lang.Integer">
            <column name="rows" length="45"/>
        </property>
        <property name="seatsEachRow" type="java.lang.Integer">
            <column name="seatsEachRow" length="45"/>
        </property>
        <property name="state" type="java.lang.Integer">
            <column name="state" length="45"/>
        </property>
    </class>
</hibernate-mapping>
